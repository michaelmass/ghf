{
  "$schema": "./schema.json",
  "presets": {
    "liscense-mit": [
      {
        "type": "file",
        "path": "LICENSE",
        "content": {
          "path": "LICENSE-MIT"
        }
      }
    ],
    "gitignore": [
      {
        "type": "lines",
        "path": ".gitignore",
        "content": {
          "path": ".gitignore"
        }
      }
    ],
    "tiltfile-init": [
      {
        "type": "part",
        "path": "Tiltfile",
        "strategy": "start",
        "content": {
          "url": "https://raw.githubusercontent.com/michaelmass/tiltfile-extensions/master/default.Tiltfile"
        }
      }
    ],
    "biome": [
      {
        "type": "file",
        "path": "biome.json",
        "content": {
          "path": "biome.txt"
        }
      }
    ],
    "github-actions": [
      {
        "type": "file",
        "path": ".github/workflows/ensure-checks-pass.yml",
        "content": {
          "path": "ensure-checks-pass.yml"
        }
      }
    ],
    "ci-deno": [
      {
        "type": "merge",
        "path": ".vscode/settings.json",
        "mergeArrays": true,
        "content": {
          "path": "vscode-settings.json"
        }
      }
    ],
    "lefthook": [
      {
        "type": "file",
        "path": "lefthook.yml",
        "content": {
          "path": "lefthook.yml"
        }
      }
    ]
  }
}
